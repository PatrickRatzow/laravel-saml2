{
    "name": "aacotroneo/laravel-saml2",
    "description": "A Laravel package for Saml2 integration as a SP (service provider) based on OneLogin toolkit, which is much lightweight than simplesamlphp",
    "license": "MIT",
    "keywords": ["laravel", "saml", "saml2", "onelogin"],
    "type": "library",
    "version": "1.0.0",
    "homepage": "https://github.com/dwarfhq/laravel-saml2",
    "authors": [
        {
            "name": "aacotroneo",
            "email": "aacotroneo@gmail.com"
        },
        {
            "name": "Frederik Sauer",
            "email": "fsa@dwarf.dk"
        }
    ],
    "require": {
        "php": ">=7.3",
        "ext-openssl": "*",
        "illuminate/config": "^8.0",
        "illuminate/support": "^8.0",
        "onelogin/php-saml": "^3.0.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "~8.0|~9.0"
    },
    "autoload": {
        "psr-4": {
            "Aacotroneo\\Saml2\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Aacotroneo\\Saml2\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Aacotroneo\\Saml2\\Saml2ServiceProvider"
            ],
            "aliases": {
                "Saml2": "Aacotroneo\\Saml2\\Facades\\Saml2"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
